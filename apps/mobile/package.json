{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration\\.test",
    "test:unit:fast": "jest --bail --onlyChanged --passWithNoTests --testPathIgnorePatterns=integration\\.test",
    "test:integration": "jest --runInBand --testPathPattern=integration\\.test",
    "test:component": "jest --runInBand --testPathPattern=components/__tests__/",
    "test:performance": "jest --runInBand --testPathPattern=__tests__/performance",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=text-summary --coverageReporters=lcov --coverageReporters=html --coverageReporters=html-spa",
    "test:coverage-check": "jest --coverage --runInBand",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:all": "./scripts/test-all.sh",
    "test:all:e2e": "./scripts/test-all.sh --e2e",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "@types/jest": "^30.0.0",
    "expo": "53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.6.1",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.28.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^53.0.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.6.2",
    "react-native-reanimated": "~3.17.4",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "private": true
}
