{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration\\.test",
    "test:unit:fast": "jest --bail --onlyChanged --passWithNoTests --testPathIgnorePatterns=integration\\.test",
    "test:integration": "jest --runInBand --testPathPattern=integration\\.test",
    "test:component": "jest --runInBand --testPathPattern=components/__tests__/",
    "test:performance": "jest --runInBand --testPathPattern=__tests__/performance",
    "test:coverage": "jest --coverage",
    "test:coverage:html": "jest --coverage --coverageReporters=text-summary --coverageReporters=lcov --coverageReporters=html --coverageReporters=html-spa",
    "test:coverage-check": "jest --coverage --runInBand",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:all": "./scripts/test-all.sh",
    "test:all:e2e": "./scripts/test-all.sh --e2e",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "expo": "54.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.6.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "19.1.13",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-jest": "^29.7.0",
    "detox": "^20.42.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expect": "29.7.0",
    "expo-doctor": "^1.17.8",
    "globals": "^16.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^54.0.12",
    "prettier": "^3.6.2",
    "react-native-reanimated": "~3.17.4",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.1",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "private": true
}
